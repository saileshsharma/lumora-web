{
  "rewrites": [
    {
      "source": "/api/**",
      "destination": "https://web-production-53490.up.railway.app/api/**"
    },
    {
      "source": "**",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "**",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
